{
  "manifest_version": 2,

  "name": "Teleport",
  "description": "Send Url to your phone right from here!",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    // Recommended
    "scripts": ["jquery-min.js"],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
